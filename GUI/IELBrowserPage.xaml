<Base:IELObject
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:Base="clr-namespace:IEL.CORE.BaseUserControls"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:IEL.GUI"
    xmlns:GUI="clr-namespace:IEL.GUI"
    x:Class="IEL.GUI.IELBrowserPage"
    mc:Ignorable="d" 
    d:DesignHeight="380.454" d:DesignWidth="487.727">
    <Control.Resources>
        <ResourceDictionary Source="/IEL;component/CORE/Resources/VisualDictionary.xaml"/>
    </Control.Resources>
    <Border x:Name="BorderMain">
        <Grid x:Name="GridMain">
            <Grid.RowDefinitions>
                <RowDefinition x:Name="RowDefinitionMainInlays" Height="65"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock x:Name="TextBlockNullPage" Text="Нет доступных вкладок" Opacity="0.4" VerticalAlignment="Center" TextAlignment="Center" Grid.Row="1"/>
            <Grid Grid.Row="1">
                <local:IELPageController x:Name="MainPageController" LeftAnimateSwitch="5,5,5,5" RightAnimateSwitch="-5,-5,-5,-5"/>
            </Grid>
            <Grid>
                <Border x:Name="BorderMainPage" Grid.Column="1" BorderThickness="0,0,0,2" BorderBrush="Black">
                    <Grid>
                        <Grid x:Name="GridMainButtons">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition x:Name="ColumnDefinitionMainInlays" Width="50"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="BorderInlays" BorderThickness="0,0,2,0" BorderBrush="Black">
                                <ScrollViewer x:Name="ScrollViewerInlays" VerticalScrollBarVisibility="Disabled" Template="{DynamicResource ScrollViewerHorizontalOnTopTemplate}" HorizontalScrollBarVisibility="Visible">
                                    <Grid x:Name="GridMainInlays">
                                    </Grid>
                                </ScrollViewer>
                            </Border>

                        </Grid>
                    </Grid>
                </Border>
            </Grid>
        </Grid>

    </Border>
</Base:IELObject>
